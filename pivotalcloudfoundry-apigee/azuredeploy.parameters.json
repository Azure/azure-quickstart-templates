{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "westus"
    },
    "baseUri": {
      "value": "https://raw.githubusercontent.com/Azure/azure-quickstart-templates/master/pivotalcloudfoundry-apigee/"
    },
    "adminUsername": {
      "value": "admin"
    },
    "adminSSHKey": {
      "value": "GEN-SSH-PUB-KEY"
    },
    "tenantID": {
      "value": "TENANT-ID"
    },
    "clientID": {
      "value": "CLIENT-ID"
    },
    "clientSecret": {
      "value": "CLIENT-SECRET"
    },
    "pivnetAPIToken": {
      "value": "PIVOTAL-NETWORK-TOKEN"
    },
    "storageAccountNamePrefixString": {
      "value": "pivotal"
    },
    "virtualNetworkName": {
      "value": "boshvnet-crp"
    },
    "subnetNameForBosh": {
      "value": "Bosh"
    },
    "NSGNameForBosh": {
      "value": "BoshSecurityGroup"
    },
    "subnetNameForCloudFoundry": {
      "value": "CloudFoundry"
    },
    "NSGNameForCF": {
      "value": "CFSecurityGroup"
    },
    "enableDNSOnDevbox": {
      "value": true
    },
    "apigeeEdge": {
      "value": "ApigeeRequired"
    },
    "apigeeDeploymentName": {
      "value": "apigee"
    },
    "apigeeAdminPassword": {
      "value": "apigee"
    },
    "apigeeAdminEmail": {
      "value": "admin@apigee.com"
    },
    "licenseFileText": {
      "value": "Apigee License"
    }
  }
}