{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string",
      "metadata": {
        "description": "Deployment location of where Apigee will be installed"
      }
    },
    "vmDnsName": {
      "type": "string"
    },
    "vmtypeprefix": {
      "type": "string",
      "defaultValue": "multinode"
    },
    "adminUserName": {
      "type": "string",
      "defaultValue": "apigeetrial"
    },
    "machineSettings": {
      "type": "object"
    },
    "sshKey": {
      "type": "securestring"
    },
    "storageAccountName": {
      "type": "string"
    },
    "ipAddresses": {
      "type": "array"
    },
    "managementPublicIPAddressName": {
      "type": "string"
    },
    "runtimePublicIPAddressName": {
      "type": "string"
    }
  },
  "variables": {},
  "resources": [],
  "outputs": {
    "runtimePublicDNSName": {
      "value": "empty",
      "type": "string"
    }
  }
}
