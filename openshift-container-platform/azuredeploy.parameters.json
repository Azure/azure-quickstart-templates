{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westus2"
        },
        "dnsZone": {
            "value": "GET-PREREQ-dnsZone"
        },
        "dnsZoneRG": {
            "value": "GET-PREREQ-dnsZoneRG"
        },
        "storageOption": {
            "value": "nfs"
        },
        "dataDiskSize": {
            "value": 1024
        },
        "enableNfsBackup": {
            "value": false
        },
        "adminUsername": {
            "value": "GEN-UNIQUE"
        },
        "openshiftPassword": {
            "value": "GEN-PASSWORD"
        },
        "sshPublicKey": {
            "value": "GEN-SSH-PUB-KEY"
        },
        "keyVaultResourceGroup": {
            "value": "GEN-KEYVAULT-RESOURCEGROUP-NAME"
        },
        "keyVaultName": {
            "value": "GEN-KEYVAULT-NAME"
        },
        "keyVaultSecretNamePullSecret": {
            "reference": "GEN-KEYVAULT-OPENSHIFT-PULLSECRET-REFERENCE"
        },
        "aadClientId": {
            "value": "GEN-AZUREAD-AKS-APPID"
        },
        "aadClientSecret": {
            "value": "GEN-AZUREAD-AKS-APPID-SECRET"
        }
    }
}
