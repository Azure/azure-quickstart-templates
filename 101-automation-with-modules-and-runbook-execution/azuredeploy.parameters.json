{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
   "parameters": {
    "UserEmail": {
      "type": "string",
      "metadata": {
        "description": "This is the email to get notify for pipeline activity."
      }
    },
    "Password": {
      "type": "string",
      "metadata": {
        "description": "This is password."
      }
    },
	 "WebAppName": {
      "type": "string",
      "metadata": {
        "description": "This is the name of the web app that may be provided dynamically after creating."
      }
    },
	"WebAppResourceGroup": {
      "type": "string",
      "metadata": {
        "description": "This is the name of the resource group in which web app resides."
      }
    }
  }
}