{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {

    "deploymentName": {
      "type": "string"
    }
  },
  "variables": {
    "templateLink": "https://raw.githubusercontent.com/javid-spektra/azure-quickstart-templates/fix/101-sig-image-version-create/101-sig-image-version-create/azuredeploy.json"
      },
  "resources": [
    {
      "name": "[parameters('deploymentName')]",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2020-10-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('templateLink')]",
          "contentVersion": "1.0.0.0"
        }
      }
    }
  ]
}
