{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "East Asia"
    },
    "newStorageAccountName": {
      "value": "yourstoragename"
    },
    "virtualNetworkName": {
      "value": "boshvnet-crp"
    },
    "subnetNameForBosh": {
      "value": "Bosh"
    },
    "subnetNameForCloudFoundry": {
      "value": "CloudFoundry"
    },
    "vmName": {
      "value": "yourvmname"
    },
    "vmSize": {
      "value": "Standard_D1"
    },
    "adminUsername": {
      "value": "yourusername"
    },
    "adminPassword": {
      "value": "yourpassword"
    },
    "enableDNSOnDevbox": {
      "value": true
    }
  }
}
