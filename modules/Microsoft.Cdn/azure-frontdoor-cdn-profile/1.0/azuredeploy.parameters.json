{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "skuName": {
            "value": "Premium_AzureFrontDoor"
        },
        "envName": {
            "value": "test"
        },
        "enableAfdEndpoint": {
            "value": true
        },
        "enableWAFPolicy": {
            "value": true
        },
        "wafPolicyMode": {
            "value": "Prevention"
        },
        "customDomains": {
            "value": [
                {
                  "hostname": "static.example.com"
                }
              ]
        },
        "origins": {
            "value": [
                {
                  "hostname": "static-src.example.com",
                  "originGroupName": "static-src-origin-group",
                  "patternsToMatch": [
                    "/*"
                  ],
                  "enabledState": true
                } 
              ]
        },
        "cdnProfileTags": {
            "value": {
                "supportGroup": "support@example.com"
              }
        },
        "eventHubName": {
            "value": "azurefrontdoorcdnlogsgabc1"
        },
        "eventHubNamespace": {
            "value": "frontdoor-cdnlog-eventhub-ns-abc1"
        },
        "eventHubLocation": {
            "value": "West US"
        }
    }
}