{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "adminUsername": {
      "type": "string"
    },
    "adminPassword": {
      "type": "secureString"
    },
    "domainName": {
      "type": "string"
    },
    "dnsPrefix": {
      "type": "string"
    }
    },
    "resources": [
        {
            "name": "createADController",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-06-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/Azure/azure-quickstart-templates/master/modules/active-directory-new-domain/0.9/azuredeploy.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "adminUsername": {
                        "value": "[parameters('adminUsername')]"
                    },
                    "adminPassword": {
                        "value": "[parameters('adminPassword')]"
                    },
                    "domainName": {
                        "value": "[parameters('domainName')]"
                    },
                    "dnsPrefix": {
                        "value": "[parameters('dnsPrefix')]"
                    }
                }
            }
        }
    ],
    "outputs": {
        "existingAdminUsername": {
            "type": "string",
            "value": "[parameters('adminUsername')]"
        },
        "existingAdminPassword": {
            "type": "secureString",
            "value": "[parameters('adminPassword')]"
        },
        "vnetSettings": {
            "type": "object",
            "value": "[reference('createADController').outputs]"
        }
    }
}