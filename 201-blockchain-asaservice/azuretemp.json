{
    "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "type": "String"
        },
        "password": {
            "type": "SecureString"
        },
        "location": {
            "type": "String"
        }
    },
    "resources": [
        {
            "type": "Microsoft.Blockchain/blockchainMembers",
            "apiVersion": "2018-06-01-preview",
            "name": "[parameters('name')]",
            "location": "[parameters('location')]",
            "tags": "[parameters('tags')]",
            "sku": {
                "name": "[parameters('sku')]",
                "tier": "[parameters('skuName')]"
            },
            "properties": {
                "protocol": "[parameters('protocol')]",
                "consensus": "[parameters('consensus')]",
                "password": "[parameters('password')]",
                "validatorNodesSku": {
                    "capacity": "[parameters('validatorNodeSkuCapacity')]"
                },
                "consortium": "[parameters('consortium')]",
                "consortiumManagementAccountPassword": "[parameters('consortiumManagementAccountPassword')]"
            }
        }
    ]
}