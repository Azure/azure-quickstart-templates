{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {

    "vms": {
      "domainControllers": {
        "vmSize": "Standard_D1_v2",
        "storageType": "Standard_LRS",
        "storageName": "GEN-UNIQUE"
      },
      "sqlServers": {
        "vmSize": "Standard_DS4",
        "storageType": "Premium_LRS",
        "storageName": "GEN-UNIQUE",
        "serviceAccountName": "sqlservice",
        "serviceAccountPassword": "GEN-PASSWORD",
        "numberOfDisks": "2",
        "workloadType": "OLTP"
      },
      "sqlWitness": {
        "vmSize": "Standard_DS1"
      }
    },
    "network": {
      "range": {
        "wholeNetwork": "10.0.0.0/16",
        "staticSubnet": "10.0.0.0/24",
        "sqlSubnet": "10.0.1.0/26"
      },
      "staticAddress": {
        "dcPrimary": "10.0.0.4",
        "dcBackup": "10.0.0.5",
        "sqlIlb": "10.0.1.9"
      }
    },

    "adminUsername": {
      "value": "alwaysonadmin"
    },
    "adminPassword": {
      "value": "GEN-PASSWORD"
    },
    "domainName": {
      "value": "contoso.local"
    },
    "autoPatchingDay": {
      "value": "Sunday"
    },
    "autoPatchingStartHour": {
      "value": "2"
    },
    "sqlAOAGName": {
      "value": "alwayson-ag"
    },
    "sqlAOListenerPort": {
      "value": "1433"
    },
    "sqlAOListenerName": {
      "value": "alwayson-ag-listener"
    },
    "sqlServerVersion": {
      "value": "SQL2014SP1-WS2012R2"
    }
  }
}