{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environment": {
      "value": "Sandbox"
    },
    "vnet_cidr": {
      "value": "10.0.0.0/16"
    },
    "subnet_waf_cidr": {
      "value": "10.0.0.0/24"
    },
    "subnet_apim_cidr": {
      "value": "10.0.1.0/24"
    },
    "apim_publisher_email": {
      "value": "damien.cuenot@live.com"
    },
    "apim_publisher_name": {
      "value": "APIM Sandbox"
    },
    "dns_zone": {
      "value": "sandbox.hostname.org"
    },
    "dns_resource_group": {
      "value": "dns-rg"
    },
    "base64_encoded_pfx_wildcard_certificate": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<subscriptionID>/resourceGroups/<resourceGroupName>/providers/Microsoft.KeyVault/vaults/<KeyVaultName>"
        },
        "secretName": "wildcard-certifcate"
      }
    }
  }
}
