{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": ""
        },
        "resourcegroup": {
            "type": "string",
            "defaultValue": ""
        },
        "subscriptionId": {
            "type": "string",
            "defaultValue": ""
        },
        "workspace": {
            "type": "string",
            "defaultValue": ""
        },
        "workspaceapiversion": {
            "type": "string",
            "defaultValue": ""
        }
    },
    "resources": [
        {
            "apiVersion": "[parameters('workspaceapiversion')]",
            "name": "[parameters('workspace')]",
            "type": "Microsoft.OperationalInsights/workspaces",
            "location": "[parameters('location')]",
            "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]",
            "resources": [
                {
                    "apiVersion": "2015-11-01-preview",
                    "name": "Web App Azure Monitoring Solution",
                    "type": "views",
                    "location": "[parameters('location')]",
                    "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'),'/views/Web App Azure Monitoring Solution')]",
                    "dependson": [
                        "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]"
                    ],
                    "properties": {
                        "Id": "Web App Azure Monitoring Solution",
                        "Name": "Web App Azure Monitoring Solution",
                        "Description": "Azure Web App Analytics",
                        "Author": "jbritt@microsoft.com",
                        "Source": "Local",
                        "Dashboard": [
                            {
                                "Id": "InformationBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "Title": "Azure Web Apps Monitoring",
                                        "NewGroup": false,
                                        "Color": "#002050"
                                    },
                                    "Header": {
                                        "Image": "data:image/png;base64,",
                                        "Label": "Web Apps Azure Monitoring",
                                        "Link": {
                                            "Label": "More info",
                                            "Url": "https://portal.azure.com"
                                        }
                                    },
                                    "List": [
                                        {
                                            "Title": "",
                                            "Content": "Out of the box monitoring for Web Apps Azure. To enable your alerting rules go to the 'Settings' page under 'Alerts' tab."
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "SingleQueryDonutBuilderBladeV1",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Web Apps Azure Alerts",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Web Apps Azure Alerts in the last 24 hours",
                                        "Subtitle": ""
                                    },
                                    "Donut": {
                                        "Query": "Type=Alert SourceSystem=OMS AlertName='WebApp*Azure*' TimeGenerated>NOW-1HOUR | measure count() by AlertSeverity",
                                        "CenterLegend": {
                                            "Text": "Total",
                                            "Operation": "Sum",
                                            "ArcsToSelect": []
                                        },
                                        "Options": {
                                            "colors": [
                                                "#ba141a",
                                                "#fff100",
                                                "#00188f"
                                            ],
                                            "valueColorMapping": []
                                        }
                                    },
                                    "List": {
                                        "Query": "Type=Alert SourceSystem=OMS AlertName='WebApp*Azure*' TimeGenerated>NOW-1HOUR | measure count() by AlertName",
                                        "HideGraph": false,
                                        "enableSparklines": false,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Alert Name",
                                            "Value": "Count"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": 60,
                                                    "color": "#fcd116"
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": 90,
                                                    "color": "#ba141a"
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "Type=Alert SourceSystem=OMS AlertName='WebApp*Azure*' TimeGenerated>NOW-1HOUR | measure count() by AlertName"
                                    }
                                }
                            },
                            {
                                "Id": "LineChartSparklineBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Web App Performance",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Web App Performance Counters",
                                        "Subtitle": ""
                                    },
                                    "lineChart": {
                                        "query": "Type=webappazure_CL MetricName_s=AverageResponseTime OR BytesSent OR BytesReceived | measure max(Average_d) by MetricName_s interval 1hour",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type=webappazure_CL MetricName_s=AverageResponseTime OR BytesSent OR BytesReceived   | measure max(Average_d) by MetricName_s interval 1hour",
                                        "operation": "Max",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "ColumnsTitle": {
                                            "Name": "Counter",
                                            "Value": "Max Value"
                                        },
                                        "Color": "#007233",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": 60,
                                                    "color": "#fcd116"
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": 90,
                                                    "color": "#ba141a"
                                                }
                                            ]
                                        },
                                        "NavigationQuery": "Type=webappazure_CL  MetricName_s!=storage && MetricName_s!=storage_percent | measure avg(Average_d) by MetricName_s interval 1hour"
                                    }
                                }
                            },
                            {
                                "Id": "LineChartSparklineBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "HTTP Requests",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Web Request Trends",
                                        "Subtitle": ""
                                    },
                                    "lineChart": {
                                        "query": "Type=webappazure_CL MetricName_s=Requests OR Http2xx OR Http3xx OR Http4xx OR Http5xx | measure max(Average_d) by MetricName_s interval 1hour",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type=webappazure_CL MetricName_s=Requests OR Http2xx OR Http3xx OR Http4xx OR Http5xx | measure max(Average_d) by MetricName_s interval 1hour",
                                        "operation": "Summary",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "ColumnsTitle": {
                                            "Name": "Counter",
                                            "Value": "Max Value"
                                        },
                                        "Color": "#e81123",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": 60,
                                                    "color": "#fcd116"
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": 90,
                                                    "color": "#ba141a"
                                                }
                                            ]
                                        },
                                        "NavigationQuery": "{selected item}"
                                    }
                                }
                            },
                            {
                                "Id": "NumberTileListBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Count of Subscriptions",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Tile": {
                                        "Query": "Type=webappazure_CL | measure countdistinct(SubscriptionId) by SubscriptionId",
                                        "Legend": "Total Subscriptions"
                                    },
                                    "List": {
                                        "Query": "Type=webappazure_CL | measure countdistinct(SubscriptionId) by AzureSubscription_s",
                                        "HideGraph": false,
                                        "enableSparklines": false,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Subscription Name",
                                            "Value": "Count"
                                        },
                                        "Color": "#00188f",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": 60,
                                                    "color": "#fcd116"
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": 90,
                                                    "color": "#ba141a"
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "{selected item}"
                                    }
                                }
                            },
                            {
                                "Id": "LineChartSparklineBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Web App Response Time ",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Web Application Response Time",
                                        "Subtitle": ""
                                    },
                                    "lineChart": {
                                        "query": "Type=webappazure_CL MetricName_s=AverageResponseTime  | measure Max(Average_d) by ServerName_s interval 10minutes",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type=webappazure_CL MetricName_s=AverageResponseTime  | measure Max(Average_d) by ServerName_s interval 10minutes",
                                        "operation": "Summary",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "ColumnsTitle": {
                                            "Name": "App Name",
                                            "Value": "Respose Time"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "60",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "90",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NavigationQuery": "{selected item}"
                                    }
                                }
                            }
                        ],
                        "OverviewTile": {
                            "Id": "DoubleNumberBuilderTile",
                            "Type": "OverviewTile",
                            "Version": 0,
                            "Configuration": {
                                "TileOne": {
                                    "Legend": "Web Apps Azure Alerts in the last hour",
                                    "Query": "Type=Alert AlertName='WebApp*Azure*' TimeGenerated>NOW-1HOURS"
                                },
                                "TileTwo": {
                                    "Legend": "Web Apps Azure Count",
                                    "Query": "Type=webappazure_CL | measure count() by ServerName_s"
                                },
                                "Advanced": {
                                    "DataFlowVerification": {
                                        "Enabled": true,
                                        "Query": "Type=webappazure_CL",
                                        "Message": "Data collection being performed.  If more than 4 hours has past, please see below troubleshooting tips.\n\nGet configuration and troubleshooting tips at https://aka.ms/webappsdoc"
                                    }
                                }
                            }
                        }
                    }
                }
            ]
        }
    ]
}
