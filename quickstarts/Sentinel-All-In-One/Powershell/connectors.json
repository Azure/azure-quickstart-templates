{
    "connectors": [
        {
            "kind": "AzureActivityLog"
        },
        {
            "kind": "AzureSecurityCenter",
            "properties": {
                "dataTypes": {
                    "alerts": {
                        "state": "enabled"
                    }
                }
            }
        },
        {
            "kind": "AzureActiveDirectoryDiagnostics",
            "properties": {
                "logs": [
                    {
                        "category": "SignInLogs",
                        "enabled": true
                    },
                    {
                        "category": "AuditLogs",
                        "enabled": true
                    }
                ]
            }
        },
        {
            "kind": "AzureActiveDirectory",
            "properties": {
                "dataTypes": {
                    "alerts": {
                        "state": "enabled"
                    }
                }
            }
        },
        {
            "kind": "Office365",
            "properties": {
                "dataTypes": {
                    "sharePoint": {
                        "state": "enabled"
                    },
                    "exchange": {
                        "state": "enabled"
                    },
                    "teams": {
                        "state": "enabled"
                    }
                }
            }
        },
        {
            "kind": "MicrosoftCloudAppSecurity",
            "properties": {
                "dataTypes": {
                    "alerts": {
                        "state": "enabled"
                    },
                    "discoveryLogs": {
                        "state": "enabled"
                    }
                }
            }
        },
        {
            "kind": "AzureAdvancedThreatProtection",
            "properties": {
                "dataTypes": {
                    "alerts": {
                        "state": "enabled"
                    }
                }
            }
        },
        {
            "kind": "MicrosoftDefenderAdvancedThreatProtection",
            "properties": {
                "dataTypes": {
                    "alerts": {
                        "state": "enabled"
                    }
                }
            }
        },
        {
            "kind": "ThreatIntelligence",            
            "properties": {
                "dataTypes": {
                    "indicators": {
                        "state": "enabled"
                    }
                }
            }
        },
        {
            "kind": "Dynamics365",
            "properties": {
                "dataTypes": {
                    "dynamics365CdsActivities": {
                        "state": "enabled"
                    }
                }
            }
        },
        {
            "kind": "IOT",
            "properties": {
                "dataTypes": {
                    "alerts": {
                        "state": "enabled"
                    }
                }
            }
        },
        {
            "kind": "MicrosoftThreatIntelligence",
            "properties": {
                "dataTypes": {
                    "bingSafetyPhishingURL": {
                        "lookbackPeriod": "30 days",
                        "state": "enabled"
                    },
                    "microsoftEmergingThreatFeed": {
                        "lookbackPeriod": "30 days",
                        "state": "enabled"
                    }
                }
            }
        },
        {
            "kind": "OfficeATP",
            "properties": {
                "dataTypes": {
                    "alerts": {
                        "state": "enabled"
                    }
                }
            }
        } 
    ]
}
