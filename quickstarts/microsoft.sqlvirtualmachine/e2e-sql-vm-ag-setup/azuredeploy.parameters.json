{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "existingVirtualNetworkName": {
            "value": "existing-vnet"
        },
        "existingVirtualNetworkResourceGroupName": {
