{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "existingMobileNetworkName": {
            "value": "GET-PREREQ-existingMobileNetworkName"
        },
        "existingSliceName": {
            "value": "GET-PREREQ-existingSliceName"
        },
        "existingDataNetworkName": {
            "value": "GET-PREREQ-existingDataNetworkName"
        },
        "serviceName": {
            "value": "Allow-all-traffic"
        },
        "serviceMaximumBitRateUplink": {
            "value": "2 Gbps"
        },
        "serviceMaximumBitRateDownlink": {
            "value": "2 Gbps"
        },
        "servicePrecedence": {
            "value": 253
        },
        "dataFlowPolicyRuleName": {
            "value": "All-traffic"
        },
        "dataFlowPolicyRulePrecedence": {
            "value": 253
        },
        "dataFlowPolicyRuleTrafficControl": {
            "value": "Enabled"
        },
        "dataFlowTemplateProtocols": {
            "value": [
                "ip"
            ]
        },
        "dataFlowTemplateDirection": {
            "value": "Bidirectional"
        },
        "dataFlowTemplateRemoteIps": {
            "value": [
                "any"
            ]
        },
        "simPolicyName": {
            "value": "Default-policy"
        },
        "totalBandwidthAllowedUplink": {
            "value": "2 Gbps"
        },
        "totalBandwidthAllowedDownlink": {
            "value": "2 Gbps"
        },
        "sessionAggregateMaximumBitRateUplink": {
            "value": "2 Gbps"
        },
        "sessionAggregateMaximumBitRateDownlink": {
            "value": "2 Gbps"
        }
    }
}