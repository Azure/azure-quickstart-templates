{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "internalNetworkName": {
      "value": "internalNetworkName"
    },
    "l3IsolationDomainName": {
      "value": "l3IsolationDomainName"
    },
    "annotation": {
      "value": "annotation"
    },
    "vlanId": {
      "value": 755
    },
    "mtu": {
      "value": 1500
    },
    "connectedIPv4Subnets": {
      "value": [
        {
          "prefix": "10.0.0.0/24",
          "annotation": "annotation"
        }
      ]
    },
    "connectedIPv6Subnets": {
      "value": [
        {
          "prefix": "2fff::/64",
          "annotation": "annotation"
        }
      ]
    },
    "staticRouteConfiguration": {
      "value": {
        "bfdConfiguration": {
          "intervalInMilliSeconds": 300,
          "multiplier": 15
        },
        "ipv4Routes": [
          {
            "prefix": "10.0.0.0/24",
            "nextHop": [
              "10.10.10.1"
            ]
          }
        ],
        "ipv6Routes": [
          {
            "prefix": "2fff::/64",
            "nextHop": [
              "3fff::1"
            ]
          }
        ],
        "extension": "NoExtension"
      }
    },
    "bgpConfiguration": {
      "value": {
        "bfdConfiguration": {
          "intervalInMilliSeconds": 300,
          "multiplier": 5
        },
        "defaultRouteOriginate": "True",
        "allowAS": 10,
        "allowASOverride": "Enable",
        "peerASN": 61234,
        "ipv4ListenRangePrefixes": [
          "10.0.0.0/25"
        ],
        "ipv6ListenRangePrefixes": [
          "2fff::/66"
        ],
        "ipv4NeighborAddress": [
          {
            "address": "10.0.0.0"
          }
        ],
        "ipv6NeighborAddress": [
          {
            "address": "2fff::"
          }
        ],
        "annotation": "annotation"
      }
    },
    "isMonitoringEnabled": {
      "value": "True"
    },
    "extension": {
      "value": "NoExtension"
    },
    "importRoutePolicy": {
      "value": {
        "importIpv4RoutePolicyId": "/subscriptions/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx/resourceGroups/example-rg/providers/Microsoft.ManagedNetworkFabric/routePolicies/example-routePolicy",
        "importIpv6RoutePolicyId": "/subscriptions/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx/resourceGroups/example-rg/providers/Microsoft.ManagedNetworkFabric/routePolicies/example-routePolicy"
      }
    },
    "exportRoutePolicy": {
      "value": {
        "exportIpv4RoutePolicyId": "/subscriptions/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx/resourceGroups/example-rg/providers/Microsoft.ManagedNetworkFabric/routePolicies/example-routePolicy",
        "exportIpv6RoutePolicyId": "/subscriptions/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx/resourceGroups/example-rg/providers/Microsoft.ManagedNetworkFabric/routePolicies/example-routePolicy"
      }
    }
  }
}