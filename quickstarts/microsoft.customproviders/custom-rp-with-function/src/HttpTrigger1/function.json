{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "methods": [
        "get",
        "delete",
        "put",
        "post"
      ]
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "table",
      "name": "tableStorage",
      "tableName": "customResource",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ]
}
