        "value": ["100.71.45.125"]
      },
    "physicalNodesSettings": {
        "value": [
            {
            "name": "serbername1",
            "ipv4Address": "100.69.32.4"
        },
        {
            "name": "servername2",
            "ipv4Address": "100.69.32.5"
        }
                 ]
      },
