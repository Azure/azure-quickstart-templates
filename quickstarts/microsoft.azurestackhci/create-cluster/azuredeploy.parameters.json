        "value": "armdeploykv"
      },
      "softDeleteRetentionDays": {
        "value": 30
      },
      "diagnosticStorageAccountName": {
        "value": ""
      },
      "logsRetentionInDays": {
        "value": 30
      },
      "storageAccountType": {
        "value": "Standard_LRS"
