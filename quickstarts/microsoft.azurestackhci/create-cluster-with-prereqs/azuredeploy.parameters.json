{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "deploymentMode": {
      "value": "Validate"
    },
    "deploymentPrefix": {
      "value": "contoso"
    },
    "localAdminUsername": {
      "value": "localadmin"
    },
    "localAdminPassword": {
      "value": "Password1234!"
    },
    "deploymentUsername": {
      "value": "deployUser"
    },
    "deploymentUserPassword": {
      "value": "GEN-UNIQUE"
    },
    "arbDeploymentAppId": {
      "value": "00000000-0000-0000-0000-000000000000"
    },
    "arbDeploymentServicePrincipalSecret": {
      "value": "<clientSecretValue>"
    },
    "hciResourceProviderObjectId": {
      "value": "00000000-0000-0000-0000-000000000000"
    },
    "clusterName": {
      "value": "contoso-cluster"
    },
    "clusterNodeNames": {
      "value": [
        "contoso-node0",
        "contoso-node1"
      ]
    },
    "domainOUPath": {
      "value": "OU=HCI,DC=contoso,DC=com"
    },
    "domainFqdn": {
      "value": "contoso.com"
    },
    "softDeleteRetentionDays": {
      "value": 30
    },
    "logsRetentionInDays": {
      "value": 30
    },
    "securityConfiguration": {
      "value": {
        "hvciProtection": true,
        "drtmProtection": true,
        "driftControlEnforced": true,
        "credentialGuardEnforced": true,
        "smbSigningEnforced": true,
        "smbClusterEncryption": true,
        "sideChannelMitigationEnforced": true,
        "bitlockerBootVolume": true,
        "bitlockerDataVolumes": true,
        "wdacEnforced": true
      }
    },
    "streamingDataClient": {
      "value": true
    },
    "isEuropeanUnionLocation": {
      "value": false
    },
    "episodicDataUpload": {
      "value": true
    },
    "storageConfigurationMode": {
      "value": "Express"
    },
    "subnetMask": {
      "value": "255.255.255.0"
    },
    "defaultGateway": {
      "value": "192.168.0.1"
    },
    "startingIPAddress": {
      "value": "192.168.0.10"
    },
    "endingIPAddress": {
      "value": "192.168.0.19"
    },
    "dnsServers": {
      "value": [
        "192.168.0.1"
      ]
    },
    "storageConnectivitySwitchless": {
      "value": false
    },
    "enableStorageAutoIp": {
      "value": true
    },
    "storageNetworks": {
      "value": [
        {
          "adapterName": "SMB0",
          "vlan": "711"
        },
        {
          "adapterName": "SMB1",
          "vlan": "712"
        }
      ]
    },
    "computeIntentAdapterNames": {
      "value": [
        "compute0",
        "compute1"
      ]
    },
    "managementIntentAdapterNames": {
      "value": [
        "management0",
        "management1"
      ]
    }
  }
}