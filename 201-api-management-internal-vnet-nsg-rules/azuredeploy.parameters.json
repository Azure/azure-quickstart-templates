{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "publisherEmail": {
            "value": "email@example.com"
        },
        "publisherName": {
            "value": "templateTest"
        },
        "proxyCustomHostname": {
            "value": "proxyHostname.contoso.com"
        },
        "proxyCustomHostnameBase64EncodedPfxCertificate": {
            "value": "<the base64 string from the pfx certificate that corresponds to the APIM Proxy custom hostname>"
        },
        "proxySSLCertificatePassword": {
            "value": "ChangeMe!"
        },
        "portalCustomHostname": {
            "value": "portalHostname.contoso.com"
        },
        "portalCustomHostnameBase64EncodedPfxCertificate": {
            "value": "<the base64 string from the pfx certificate that corresponds to the APIM Portal custom hostname>"
        },
        "portalSSLCertificatePassword": {
            "value": "ChangeMe!"
        },
        "virtualNetworkName": {
            "value": "apim-internal-vnet"
        },
        "subnetName": {
            "value": "apim-internal-subnet"
        }
    }
}
