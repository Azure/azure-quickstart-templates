{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    
        "location": {
            "value": "West US"
        },
        "dnsNameForPublicIP": {
          "value": "uniquedns",
          "metadata": {
             "description": "it should match with the following regular expression: ^[a-z][a-z0-9-]{1,61}[a-z0-9]$ or it will raise an error" 
          }
        },
        "existingVirtualNetworkName": {
            "value": "yourvnet"
        },
        "existingVirtualNetworkResourceGroup": {
            "value": "VNETresourceGroup"
        },
        "subnetName": {
            "value": "VNETsubnetName"
        },

        "osDiskVhdUri": {
            "value": "customvhdlocation"
        },
        "vmName": {
          "value": "NameOfVM"
        },

        "osType": {
            "value": "Windows"
        },
        "vmSize": {
            "value": "Standard_DS1"
        }
    }
}
