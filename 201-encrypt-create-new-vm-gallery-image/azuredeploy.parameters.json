{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmName": {
      "value": "vmName"
    },
    "adminUsername": {
      "value": "userName"
    },
    "adminPassword": {
      "value": "YourSecurePassword"
    },
    "newStorageAccountName": {
      "value": "uniqueStorageAccount"
    },
    "vmStorageContainerName": {
      "value": "uniqueContainer"
    },
    "vmSize": {
      "value": "Standard_D2"
    },
    "virtualNetworkName": {
      "value": "uniqueVnet"
    },
    "subnetName": {
      "value": "Subnet"
    },
    "aadClientID": {
      "value": "clientId"
    },
    "aadClientSecret": {
      "value": "YourClientSecret"
    },
    "keyVaultName": {
      "value": "kvName"
    },
    "keyVaultResourceGroup": {
      "value": "kvResourceGroup"
    },
    "useExistingKek": {
      "value": "nokek"
    },
    "keyEncryptionKeyURL": {
      "value": "kekUrl"
    }
  }
}
