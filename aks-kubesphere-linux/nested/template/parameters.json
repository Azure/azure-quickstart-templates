{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourceName": {
            "value": "AKS-KubeSphere"
        },
        "location": {
            "value": "chinanorth2"
        },
        "dnsPrefix": {
            "value": "AKS-KubeSphere-dns"
        },
        "kubernetesVersion": {
            "value": "1.18.14"
        },
        "networkPlugin": {
            "value": "kubenet"
        },
        "enableRBAC": {
            "value": true
        },
        "enablePrivateCluster": {
            "value": true
        },
        "enableAzurePolicy": {
            "value": false
        },
        "vmssNodePool": {
            "value": true
        },
        "networkPolicy": {
            "value": "calico"
        }
    }
}