{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "adminUsername": {
            "value": "GEN-USERNAME"
        },
        "sshPublicKey": {
            "value": "GEN-SSH-PUB-KEY"
        },
        "devopsDnsPrefix": {
            "value": "GEN-UNIQUE"
        },
        "servicePrincipalAppId": {
            "value": "GEN-UNIQUE-18"
        },
        "servicePrincipalAppKey": {
            "value": "GEN-PASSWORD"
        },
        "gitRepository": {
            "value": "https://github.com/lwander/spin-kub-demo.git"
        },
        "dockerRepository": {
            "value": "lwander/spin-kub-demo"
        },
        "registrySku": {
            "defaultValue": "Basic",
            "type": "String",
            "metadata": {
                "description": "Sku of the container registry"
            }
        },
        "registryApiVersion": {
            "defaultValue": "2017-03-01",
            "type": "String",
            "metadata": {
                "description": "Api version of the container registry"
            }
        },
    }
}