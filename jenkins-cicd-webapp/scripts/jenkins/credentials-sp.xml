<com.microsoft.azure.util.AzureCredentials plugin="azure-credentials@1.4.0">
  <scope>GLOBAL</scope>
  <id>{insert-credentials-id}</id>
  <description>{insert-credentials-description}</description>
  <data>
    <subscriptionId>{insert-subscription-id}</subscriptionId>
    <clientId>{insert-client-id}</clientId>
    <clientSecret>{insert-client-secret}</clientSecret>
    <tenant>{insert-tenant-id}</tenant>
    <azureEnvironmentName>Azure</azureEnvironmentName>
  </data>
</com.microsoft.azure.util.AzureCredentials>