{
    "views": [
        {
            "kind": "Overview",
            "properties": {
                "header": "Welcome to your managed application with metrics.",
                "description": "Hello Managed Application metrics! Go to Metrics menu to see application storage account availability, E2E latency and transactions metrics. This Managed application is for demo purposes only. "
            }
        },
        {
            "kind": "Metrics",
            "properties": {
                "timeSpan": {
                    "relative": {
                        "durationMs": 86400000
                    }
                }
            },
            "charts": [
                {
                    "displayName": "Availability of application storage account",
                    "chartType": "Bar",
                    "metrics": [
                        {
                            "resourceType": "Microsoft.Storage/storageAccounts",
                            "name": "Availability",
                            "aggregationType": "Avg"
                        }
                    ]
                },
                {
                    "displayName": "Success E2E Latency of application storage account",
                    "metrics": [
                        {
                            "resourceType": "Microsoft.Storage/storageAccounts",
                            "name": "SuccessE2ELatency",
                            "aggregationType": "Avg"
                        }
                    ]
                },
                {
                    "displayName": "Transactions of application storage account",
                    "metrics": [
                        {
                            "resourceType": "Microsoft.Storage/storageAccounts",
                            "name": "Transactions",
                            "aggregationType": "Sum"
                        }
                    ]
                }
            ]
        }
    ]
}