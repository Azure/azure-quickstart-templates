{
    "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "value": "West Europe"
        },
        "storageAccountName": {
            "type": "string",
            "value": "dockerswarmstorage"
        },
        "virtualNetworkName": {
            "type": "string",
            "value": "dockerSwarmVnet"
        },
        "virtualNetworkPrefix": {
            "type": "string",
            "value": "10.0.0.0/8"
        },
        "subnetSwarmName": {
            "type": "string",
            "value": "Swarm-Subnet"
        },
        "subnetSwarmPrefix": {
            "type": "string",
            "value": "10.0.1.0/24"
        },
        "subnetManageName": {
            "type": "string",
            "value": "Manage-Subnet"
        },
        "subnetManagePrefix": {
            "type": "string",
            "value": "10.0.0.0/24"
        },
        "swarmNodesNumber": {
            "type": "int",
            "value": 3
        },
        "swarmClusterNodesSize": {
            "type": "string",
            "value": "Standard_A1"
        },
        "adminUsername": {
            "type": "string",
            "value": "swarmAdmin"
        },
        "adminPassword": {
            "type": "securestring",
            "value": "Passw0rd!"
        },
        "swarmMasterPublicIpDnsName": {
            "type": "string",
            "value": "dockerswarmcluster"

        },
        "swarmMasterPublicIpName": {
            "type": "string",
            "value": "swarmMasterPublicIp"
        },
        "scriptsUri": {
            "type": "string",
            "value": "https://raw.githubusercontent.com/azure/azure-quickstart-templates/master/docker-swarm-cluster/"
        }

    }
}