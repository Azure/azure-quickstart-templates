{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
  },
  "variables": {
    "firstVNETName": "vnet01",
    "firstVNETPrefix": "10.0.0.0/16",
    "firstVNETFESubnetName": "FE-Subnet",
    "firstVNETFESubnetPrefix": "10.0.0.0/24",
    "firstVNETBESubnetName": "BE-Subnet",
    "firstVNETBESubnetPrefix": "10.0.1.0/24",
    "firstVNETGatewaySubnetName": "GatewaySubnet",
    "firstVNETGatewaySubnetPrefix": "10.0.254.0/29",
    "vnetID": "[resourceId('Microsoft.Network/virtualNetworks',variables('firstVNETName'))]"
  },
  "resources": [
    {
      "name": "[variables('firstVNETName')]",
      "type": "Microsoft.Network/virtualNetworks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2015-06-15",
      "dependsOn": [ ],
      "tags": {
        "displayName": "firstVNET"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('firstVNETPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('firstVNETFESubnetName')]",
            "properties": {
              "addressPrefix": "[variables('firstVNETFESubnetPrefix')]"
            }
          },
          {
            "name": "[variables('firstVNETBESubnetName')]",
            "properties": {
              "addressPrefix": "[variables('firstVNETBESubnetPrefix')]"
            }
          },
          {
            "name": "[variables('firstVNETGatewaySubnetName')]",
            "properties": {
              "addressPrefix": "[variables('firstVNETGatewaySubnetPrefix')]"
            }
          }
        ]
      }
    }
  ],
  "outputs": {
    "existingVnetName": {
      "type": "string",
      "value": "[variables('firstVNETName')]"
    },
    "existingVnetLocation": {
      "type": "string",
      "value": "[resourceGroup().location]"
    },
      "existingSubnetResourceId": {
      "type": "string",
      "value": "[concat('/subscriptions/', subscription().subscriptionId, '/resourceGroups/', resourceGroup().name,'/providers/Microsoft.Network/virtualNetworks/', variables('firstVNETName'),'/subnets/', variables('firstVNETFESubnetName'))]"
    }
  }
}
