{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "East US"
        },
        "aseName": {
            "value": "myApp-ase"
        },
        "dnsSuffix": {
            "value": "ase.contoso.com"
        },
        "vnetResourceGroupName": {
            "value": "applicationEnvironment-rg"
        },
        "vnetResourceName": {
            "value": "myAppEnvironment-vnet"
        },
        "application_name": {
            "value": "myApp-app"
        },
        "internalLoadBalancingMode": {
            "value": 3
        },
        "aseSubnetName": {
            "value": "ase-subnet"
        },
        "serverfarms_ASE_asp_name": {
            "value": "myApp-ase-asp"
        },
        "vnetAddressRange": {
            "value": "10.10.0.0/16"
        },
        "aseSubnetAddressRange": {
            "value": "10.10.0.0/24"
        },
        "aseNSGName": {
            "value": "ase-NSG"
        },
        "aseRouteTableName": {
            "value": "ase-RouteTable"
        },
        "azureFirewallName": {
            "value": "azureASEFirewall"
        },
        "azureFirewallSubnetAddressRange": {
            "value": "10.10.1.0/24"
        },
        "azureFirewallRouteTableName": {
            "value": "azureASEFirewall-RouteTable"
        },
        "azureFirewallPublicIP": {
            "value": "azureASEFirewall-pip"
        },
        "deployAzureFirewall": {
            "value": true
        },
        "tags": {
            "value": {
                "tag1": "value1",
                "tag2": "value2"
            }
        },
        "deployToAzGov": {
            "value": false
        }
    }
}