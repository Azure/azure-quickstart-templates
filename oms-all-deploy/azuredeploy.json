        {
          "apiVersion": "2015-11-01-preview",
          "type": "datasources",
          "name": "LogicalDisk6",
          "dependsOn": [
            "[concat('Microsoft.OperationalInsights/workspaces/', parameters('omsWorkspaceName'))]"
          ],
          "kind": "WindowsPerformanceCounter",
          "properties": {
            "objectName": "LogicalDisk",
            "instanceName": "*",
            "intervalSeconds": 10,
            "counterName": "Disk Writes/sec"
