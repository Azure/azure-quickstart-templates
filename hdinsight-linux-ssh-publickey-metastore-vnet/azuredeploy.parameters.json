{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "East US"
        },
        "clusterName": {
            "value": "myCluster"
        },
        "clusterVersion": {
            "value": "3.2"
        },
        "headNodeSize": {
            "value": "Standard_D3"
        },
        "workerNodeSize": {
            "value": "Standard_D3"
        },
        "workerNodeCount": {
            "value": 4
        },
        "clusterLoginUserName": {
            "value": "admin"
        },
        "clusterLoginPassword": {
            "value": "<password here>"
        },
        "sshUserName": {
            "value": "sshuser"
        },
        "sshPassword": {
            "value": "<password here>"
        },
        "existingClusterStorageResourceGroup": {
            "value": "mystorageresourcegroup"
        },
        "existingClusterStorageAccountName": {
            "value": "mystorageaccount"
        },
        "newOrExistingClusterStorageContainerName": {
            "value": "containerName"
        },
        "existingHiveMetastoreServerName": {
            "value": "servername.database.windows.net"
        },
        "existingHiveMetastoreDatabaseName": {
            "value": "databasename"
        },
        "existingHiveMetastoreUsername": {
            "value": "<metastore username>"
        },
        "existingHiveMetastorePassword": {
            "value": "<metastore password>"
        },
        "existingVirtualNetworkResourceGroup": {
            "value": "virtualnetworkresourcegroup"
        },
        "existingVirtualNetworkName": {
            "value": "virtualnetworkname"
        },
        "existingVirtualNetworkSubnetName": {
            "value": "subnetname"
        }
    }
}

