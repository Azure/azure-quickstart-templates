{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "ipConfig": {
            "type": "string"
        },
        "ipAddresses": {
            "type": "array",
            "defaultValue": []
        }
    },
    "variables": {},
    "resources": [],
    "outputs": {
        "result": {
            "type": "array",
            "value": "[concat(parameters('ipAddresses'),split(parameters('ipConfig'),','))]"
        }
    }
}
