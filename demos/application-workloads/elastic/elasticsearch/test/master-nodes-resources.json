{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminUsername": {
      "type": "string",
      "metadata": {
        "description": "Admin username used when provisioning virtual machines"
      }
    },
    "authSet": {
      "type": "object",
      "metadata": {
        "description": "Authentication information"
      }
    },
    "storageSettings": {
      "type": "object",
      "metadata": {
        "description": "Storage Account Settings"
      }
    },
    "subnet": {
      "type": "object",
      "metadata": {
        "description": "The VNET and Subnet to deploy the nodes in to"
      }
    },
    "masterNodesIpPrefix": {
      "type": "string",
      "metadata": {
        "description": "IP Prefix used to append index for static addresses"
      }
    },
    "vmSize": {
      "type": "string",
      "defaultValue": "Standard_A0",
      "metadata": {
        "description": "Size of the Elasticsearch master nodes"
      }
    },
    "osSettings": {
      "type": "object",
      "metadata": {
        "description": "Elasticsearch deployment platform settings"
      }
    },
    "namespace": {
      "type": "string",
      "metadata": {
        "description": "Namespace for resources created by this template"
      }
    }
  },
  "variables": {},
  "resources": [],
  "outputs": {
    "adminUsername": {
      "type": "string",
      "value": "[parameters('adminUsername')]"
    },
    "authSet": {
      "type": "object",
      "value": "[parameters('authSet')]"
    },
    "storageSettings": {
      "type": "object",
      "value": "[parameters('storageSettings')]"
    },
    "subnet": {
      "type": "object",
      "value": "[parameters('subnet')]"
    },
    "masterNodesIpPrefix": {
      "type": "string",
      "value": "[parameters('masterNodesIpPrefix')]"
    },
    "vmSize": {
      "type": "string",
      "value": "[parameters('vmSize')]"
    },
    "osSettings": {
      "type": "object",
      "value": "[parameters('osSettings')]"
    },
    "namespace": {
      "type": "string",
      "value": "[parameters('namespace')]"
    }
  }
}
