{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "virtualNetworkName": {
      "value": "yourvnetname"
    },
    "vnetAddressPrefix": {
      "value": "10.0.0.0/16"
    },
    "subnetName": {
      "value": "yoursubnet"
    },
    "subnetPrefix": {
      "value": "10.0.0.0/24"
    },
    "applicationGatewayName": {
      "value": "yourappgateway"
    },
    "applicationGatewaySize": {
      "value": "Standard_Small"
    },
    "applicationGatewayInstanceCount": {
      "value": 1
    },
    "frontendPort": {
      "value": 80
    },
    "backendPort": {
      "value": 80
    },
    "backendIPAddresses": {
      "value": [
        {
          "IpAddress": "10.0.0.7"
        },
        {
          "IpAddress": "10.0.0.8"
        },
        {
          "IpAddress": "10.0.0.9"
        }
      ]
    },
    "cookieBasedAffinity": {
      "value": "Disabled"
    }
  }
}