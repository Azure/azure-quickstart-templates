{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "addressPrefix": {
      "value": "10.0.0.0/16"
    },
    "subnetPrefix": {
      "value": "10.0.0.0/28"
    },
    "applicationGatewaySize": {
      "value": "Standard_Medium"
    },
    "capacity": {
      "value": 2
    },
    "backend1": {
      "value": "backend1"
    },
    "healthyStatusCodes": {
      "value": "200-399"
    },
    "probeEnabled": {
      "value": true
    },
    "probeProtocol": {
      "value": "http"
    },
    "probeHost": {
      "value": "probehost.contoso.com"
    },
    "probePath": {
      "value": "/"
    },
    "probeIntervalInSeconds": {
      "value": 30
    },
    "probeTimeoutInSeconds": {
      "value": 20
    },
    "probeUnhealthyThreshold": {
      "value": 3
    },
    "probePickHostNameFromBackendHttpSettings": {
      "value": false
    },
    "probeMinServersAvailable": {
      "value": 0
    }
  }
}