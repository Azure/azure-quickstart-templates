                            "LineChart": {
                                "Query": "Type:Perf (ObjectName=\"KempLM-TPS\") CounterName=\"Total TPS\" | measure max(CounterValue) by Computer | display linechart",
                                "yAxis": {
                                    "isLogarithmic": false,
