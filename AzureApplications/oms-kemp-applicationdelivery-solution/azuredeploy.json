                "OverviewTile": {
                    "Id": "DoubleNumberBuilderTile",
                    "Type": "OverviewTile",
                    "Version": 0,
                    "Configuration": {
                        "TileOne": {
                            "Legend": "Kemp Devices",
                            "Query": "Type:KempDevice_CL | measure countdistinct(Computer) by Computer"
